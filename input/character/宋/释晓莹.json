{
    "dynasty": "宋",
    "author": "释晓莹",
    "poems": [
        {
            "title": "句",
            "content": "临岐强破颜。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "是身多落寞。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "断岭云归白。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "何妨语夜阑。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "颓然只任真。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "衣弊履穿不讳穷。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "南屏寄傲且今年，况复相羊得自便。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "对景由来诗句丽。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "竟日霏微雨。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "漏鼓乍听连岸响。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "江城今可徘徊处。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "谁与共平章。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "胸中历历古兼今。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "脉脉受风斜，野望迷青草。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "如公自是鉴坡具。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "倦客独携筇。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "习习和风至。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "江表人材独数公。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "随宜领览到斜晖。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "放怀端与世情疏。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "素抱与谁倾。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "平章风物诗无敌。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "几回相送复相迎。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "北山迤逦自苍然。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "抚事心犹壮。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "即看青女夜飞霜。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "丰城千里外。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "灯火照青编。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "檐雨滴秋残旅梦。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "回看苍木杪。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "薄饭粗缯老此生。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "谁知老子懒成癖。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "素王妙道况亲传。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "欲罢不能聊一吟。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "蝉噪日将落。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "顾我於兹诗有兴。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "倚栏哦五字。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "寒窗安著清相向。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "贪看春山如许青。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "诗书论议深。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "绊身何用尚浮名。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "披云登断岭。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "稍稍竹过墙。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "甘棠不翦向来荣。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "倒拄乌藤乘逸兴。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "幽花欹照水。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "一言知具眼。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "青春作伴日同行。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "由来八座尚书贵。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "殷勤供一笑。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "天教春信到寒乡。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "不到兹亭久。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "绕崖云惨惨，浥屦露团团。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "落涧水淙淙。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "徘徊幽兴熟。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "故人携客作幽寻。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "白鸟翩翩接翅飞。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "栏干聊小凭，取次缀诗篇。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "桑椹累累紫。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "泉石惬幽情。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "对景觅诗联。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "不妨谈笑又诗成。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "石壁巉然不可攀。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "独冒烟岚促去程。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "高怀不我弃。",
            "comment": ""
        },
        {
            "title": "南昌道中",
            "content": "东游心渺渺，趼足步迟迟。马驿黄茅路，人家白竹篱。归云横断岭，落日映荒陂。何地可容足，长哦招隐诗。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "练练长江静。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "苍生从此得伸眉。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "久客欣无事。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "闲来湖上立移时。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "在路日已久。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "那敢谓长城。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "搘筇聊小立。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "谁教併向此中见。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "莫笑吾侬躭此癖。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "平生只信缘。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "诗成聊疥壁。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "及瓜一笑上浮艎。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "藜杖赋归休。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "寄目悠然上杳冥，好是园林争秀发。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "江上闲吟立又行。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "而今总入老夫诗。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "谁复知予懒是真。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "余思草萋萋。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "前村鸦噪断。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "自知潦倒无余事。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "路傍官河一带长。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "岸曲水流寒。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "有恨春将晚。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "陶泛一船东。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "春光和暖恰相宜。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "无因从发药。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "宁免鬓毛苍。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "山川元历历。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "可怜赢得鬓成霜。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "风沙轻漠漠。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "但觉旅怀开。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "雁字联联写碧空。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "凭栏取次看。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "清泉槛外流。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "门外黄尘扑面飞。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "偶来清赏曲江头。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "出门纵步细哦诗。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "挥犀闻妙论。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "江云黯黯作愁阴。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "舟横渔唱晚。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "与君频邂逅，此地久盘桓。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "秋色偏宜晚望中。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "二月春方好。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "婆娑弄翰墨。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "放怀便所便。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "携朋上故基。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "日色今朝分外明。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "晚来供望眼。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "不记登临日几回。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "雅淡天姿固可知。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "谈笑会当同。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "细柳乱摇风。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "地去秉钧余咫尺。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "负暄孤坐松根石。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "未厌长为客。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "发兴真成继庾公。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "可堪时序苦侵寻，留滞谁知此日心。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "隆寒正北风，飘飘任此身。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "悠悠落日悬。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "波中船舫来还去。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "寂寂饶孤兴。",
            "comment": ""
        }
    ]
}