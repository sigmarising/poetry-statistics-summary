{
    "dynasty": "宋",
    "author": "黄元道",
    "poems": [
        {
            "title": "赠邓珪",
            "content": "柳绿桃红春昼永，家童唤起睡中忙。偶然日下音书至，回首江城又夕阳。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "巽岭直下梅家店，福禄难过丑年春。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "二五相逢路再迷。",
            "comment": ""
        }
    ]
}